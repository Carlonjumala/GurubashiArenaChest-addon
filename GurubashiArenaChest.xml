<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
    https://github.com/tekkub/wow-ui-source/blob/live/FrameXML/UI.xsd">
    
    <Frame name="ChestTimerFrame" parent="UIParent" hidden="true">
        <Size>
            <AbsDimension x="200" y="100" />
        </Size>
        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <FontString name="ChestTimerText" inherits="GameFontNormal">
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                    <Text>Chest Timer Addon Loaded</Text>
                </FontString>
            </Layer>
        </Layers>
    </Frame>
</Ui>
